<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Douyu.Barrage.Collector.Properties.Douyu" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
      <section name="Douyu.Barrage.Collector.Douyu" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Douyu.Barrage.Collector.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>


  <log4net>
    <root>
      <level value="INFO"></level>
      <appender-ref ref="app"></appender-ref>
      <appender-ref ref="console"></appender-ref>"
    </root>

    <logger name="Debug">
      <level value="INFO"></level>
      <appender-ref ref="debug"></appender-ref>
    </logger>

    <logger name="Error">
      <level value="INFO"></level>
      <appender-ref ref="error"></appender-ref>
    </logger>

    <logger name="Message">
      <level value="INFO"></level>
      <appender-ref ref="message"></appender-ref>
    </logger>

    <!--输出到文件: app.log-->
    <appender name="app" type="log4net.Appender.RollingFileAppender">
      <file value="logs/app.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="7" />
      <maximumFileSize value="100MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <!--输出到文件: debug.log-->
    <appender name="debug" type="log4net.Appender.FileAppender">
      <file value="logs/debug.log" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <!--输出到文件: error.log-->
    <appender name="error" type="log4net.Appender.FileAppender">
      <file value="logs/error.log" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <!--输出到文件: mesage.log-->   
    <appender name="message" type="log4net.Appender.RollingFileAppender">
      <file value="logs/message.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="7" />
      <maximumFileSize value="100MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline" />
      </layout>
    </appender>

    <!--输出到控制台-->
    <appender name="console" type="log4net.Appender.ColoredConsoleAppender">
      <mapping>
        <level value="ERROR"/>
        <foreColor value="White"/>
        <backColor value="Red, HighIntensity"/>
      </mapping>
      <mapping>
        <level value="DEBUG"/>
        <backColor value="Green"/>
      </mapping>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
  </log4net>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  
  <applicationSettings>
    <Douyu.Barrage.Collector.Properties.Douyu>
      <setting name="MovieDir" serializeAs="String">
        <value/>
      </setting>
    </Douyu.Barrage.Collector.Properties.Douyu>
    <Douyu.Barrage.Collector.Douyu>
      <setting name="MovieDir" serializeAs="String">
        <value/>
      </setting>
    </Douyu.Barrage.Collector.Douyu>
  </applicationSettings>
  
 <runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="EntityFramework" publicKeyToken="B77A5C561934E089" culture="neutral"/>
				<bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>
 <userSettings>
  <Douyu.Barrage.Collector.Properties.Settings>
   <setting name="SavedRoom" serializeAs="String">
    <value>122402</value>
   </setting>
  </Douyu.Barrage.Collector.Properties.Settings>
 </userSettings>
</configuration>
